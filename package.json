  },
  "homepage": "https://github.com/ultimate-pagination/react-ultimate-pagination-material-ui#readme",
  "peerDependencies": {
    "material-ui": "^1.0.0-alpha.12"
  },
  "dependencies": {
    "postinstall-build": "^3.0.1",
    "react-ultimate-pagination": "1.0.1"
  },
  "devDependencies": {
    "babel-cli": "^6.7.7",
    "babel-core": "^6.7.6",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.6.0",
