    "build:umd": "webpack --output-filename react-ultimate-pagination-material-ui.js",
    "build:umd.min": "webpack --output-filename react-ultimate-pagination-material-ui.min.js -p",
    "postinstall": "postinstall-build --only-as-dependency build \"npm run build\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ultimate-pagination/react-ultimate-pagination-material-ui.git"
